{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": ["src/components/**/*.ts"],
    "extension": [".ts"],
    "branches": 50,
    "lines": 50,
    "functions": 50,
    "statements": 50,
    "watermarks": {
        "lines": [70, 80, 95],
        "functions": [70, 80, 95],
        "branches": [70, 80, 95],
        "statements": [70, 80, 95]
    }
}
